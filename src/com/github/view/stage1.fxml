<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane stylesheets="@style.css" alignment="center" hgap="10" prefHeight="230.0" prefWidth="129.0" vgap="10"
          xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="com.github.controller.Stage1Controller">
   <columnConstraints>
      <ColumnConstraints/>
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0"/>
      <RowConstraints/>
   </rowConstraints>
   <Button mnemonicParsing="false" onAction="#simulation" prefHeight="25.0" prefWidth="88.0" text="Simulation"
           GridPane.halignment="CENTER" GridPane.hgrow="NEVER" GridPane.rowIndex="1" GridPane.valignment="TOP"
           GridPane.vgrow="NEVER"/>
   <Button mnemonicParsing="false" onAction="#taxiInterface" text="Taxi Interface" GridPane.halignment="CENTER"
           GridPane.hgrow="NEVER" GridPane.valignment="TOP" GridPane.vgrow="NEVER"/>
</GridPane>
