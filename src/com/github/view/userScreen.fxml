                     <children>
                        <Label layoutX="31.0" layoutY="25.0" prefHeight="17.0" prefWidth="184.0" text="Your Balance:" />
                        <Label layoutX="31.0" layoutY="209.0" prefHeight="17.0" prefWidth="184.0" text="Add credit:" />
