                        <Label fx:id="balance" alignment="CENTER" layoutX="31.0" layoutY="57.0" prefHeight="111.0" prefWidth="312.0" text="Label" textAlignment="CENTER">
                           <font>
                              <Font size="60.0" />
                           </font>
                        </Label>
                        <ScrollPane fx:id="resultsContainer" layoutX="386.0" layoutY="49.0" prefHeight="337.0" prefWidth="358.0">
